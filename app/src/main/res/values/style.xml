        <!-- NavigationView related styling -->
        <item name="highlightColor">@color/lightHighlightColor</item>
        <item name="android:listDivider">@color/lightListDivider</item>

        <!-- SwitchCompat related styling -->
        <item name="darkModeSwitch">false</item>

