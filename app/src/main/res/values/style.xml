        <item name="android:fontFamily">@font/poppins</item>

    </style>

    <!-- Generalized Light Theme -->
    <style name="LightTheme" parent="MyTheme">

        <!-- Text related styling -->
        <item name="textColor">@color/darkTextColor</item>
